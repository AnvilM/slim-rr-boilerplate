{
    "name": "anvilm/slim-rr-boilerplate",
    "description": "Slim 4, RoadRunner boilerplate",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
          "name": "anvilm",
          "homepage": "https://github.com/AnvilM"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "slim/slim": "4.*",
        "nyholm/psr7": "^1.8",
        "php-di/php-di": "^7.1",
        "robmorgan/phinx": "^0.16.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.4",
        "phpstan/phpstan": "^2.1"
    }
}
